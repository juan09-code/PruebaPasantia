<h1 class="text-titles">Notas por Estudiante<small></small></h1>
<div class="table-responsive">
	<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
		<thead>

			<tr>
				<th scope="col">CI</th>
				<th scope="col">Nombres y Apellidos</th>
				<th scope="col">N1</th>
				<th scope="col">N2</th>
				<th scope="col">N3</th>
				<th scope="col">N4</th>
				<th scope="col">N5</th>
				<th scope="col">1P</th>
				<th scope="col">N6</th>
				<th scope="col">N7</th>
				<th scope="col">N8</th>
				<th scope="col">N9</th>
				<th scope="col">N10</th>
				<th scope="col">2P</th>
				<th scope="col">80% </th>
				<th scope="col">Ex.</th>
				<th scope="col">20% </th>
				<th scope="col">Final</th>
				<th scope="col">Estado</th>

			</tr>
		</thead>
		<tbody>
			{{#each notas}}
			<tr>
				<th scope="row">{{cedula}}</th>
				<td>{{nombres}} {{apellidos}}</td>
				<td>{{nota1}}</td>
				<td>{{nota2}}</td>
				<td>{{nota3}}</td>
				<td>{{nota4}}</td>
				<td>{{nota5}}</td>
				<td>{{promedio1}}</td>
				<td>{{nota6}}</td>
				<td>{{nota7}}</td>
				<td>{{nota8}}</td>
				<td>{{nota9}}</td>
				<td>{{nota10}}</td>
				<td>{{promedio2}}</td>
				<td>{{nota_80}}</td>
				<td>{{examen}}</td>
				<td>{{examen_20}}</td>
				<td>{{promedio_f}}</td>
				<td>{{estado}}</td>
			</tr>
			{{/each}}
		</tbody>
	</table>
</div>



